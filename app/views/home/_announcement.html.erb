<% case announcement.category %>
<% when Announcement::NOTICE%>
    <td><i class="far fa-star"></i>
    <td class="notice_type">
<% when Announcement::MAINTENANCE%>
    <td><i class="fas fa-wrench"></i>
    <td class="notice_type" style="color: orange">
<% when Announcement::IMPORTANT%>
    <td><i class="fas fa-exclamation-circle"></i>
    <td class="notice_type" style="color: red">
<% when Announcement::STATUS%>
    <td><i class="fas fa-times-circle"></i>
    <td class="notice_type" style="color: red">
<%end%>
[<%= announcement.category_name %>]</td>
<td><%= announcement.title %></td>
<td class="notice_date" ><%= announcement.updated_at.strftime("%F") %></td>

